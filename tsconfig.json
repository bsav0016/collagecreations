{
    "compilerOptions": {
        "outDir": "./dist",
        "target": "es5",
        "module": "esnext",
        "moduleResolution": "node", 
        "jsx": "react-jsx",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "allowJs": true,
        "resolveJsonModule": true
    },
    "include": [
        "src/**/*"
    ],
    "exclude": [
        "node_modules"
    ]
}
  